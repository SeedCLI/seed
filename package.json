{
	"name": "seedcli",
	"version": "1.0.0",
	"description": "Batteries-included, modular, TypeScript-first CLI framework powered by Bun",
	"license": "MIT",
	"author": "Rully Ardiansyah <rully@dreamshive.io>",
	"repository": {
		"type": "git",
		"url": "https://github.com/SeedCLI/seed"
	},
	"homepage": "https://seedcli.dev",
	"engines": {
		"bun": ">=1.3.9"
	},
	"private": true,
	"workspaces": [
		"packages/*",
		"examples/*"
	],
	"scripts": {
		"build": "bun scripts/build.ts",
		"build:clean": "bun scripts/build.ts --clean",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"format": "biome format --write .",
		"test": "bun test",
		"typecheck": "tsc --noEmit",
		"publish:dry": "bun scripts/publish.ts --dry-run",
		"publish:npm": "bun scripts/publish.ts"
	},
	"devDependencies": {
		"@biomejs/biome": "2.4.4",
		"bun-types": "1.3.9",
		"openapi-fetch": "^0.17.0",
		"typescript": "5.9.3"
	}
}
